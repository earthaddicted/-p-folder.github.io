extends root

block content
	+header({header: globals.header, menu: globals.menu})

	main.main.leaders-page
		block main
			+b('section').section._light
				+e.wrapper
					+b-breadcrumbs()

			+b('section').section._light._leaders
				+e.wrapper.section__wrapper-col
					+b-leaders({leaders: globals.leaders})

			+b('section').section._light._contacts
				+e.wrapper
					+b-contacts-small()
					+b-contacts-popup(data)

	+footer({footer: globals.footer, menu: globals.menu})
