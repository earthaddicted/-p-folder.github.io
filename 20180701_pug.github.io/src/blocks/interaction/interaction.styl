		.block-multiple-wrapper
			display flex
			flex-wrap wrap
			align-items center
			margin: -280px auto 0
			margin: -100px auto 0
			justify-content space-between
			position relative

			@media screen and (max-width: 1100px)
				margin-top: -142px
				margin-top: -231px
				width: 80%

			@media screen and (max-width: 1100px)
				margin-top: -100px;

			@media screen and (max-width: 720px)
				flex-direction: column
				width auto
				min-width 310px
				margin-top: 20px
				border: 3px solid #25c8f1
				border-radius 10px
				margin-left: 20px
				padding-bottom 50px

			@media screen and (max-width: 500px)
				min-width 250px

			&__special
				.col-text
					display: flex
					align-items: flex-end
					padding-right: 5px
					min-width 320px

					@media screen and (max-width: 900px)
						max-width: 175px
						min-width: 175px
						width: 175px;

					h3
						font-size 36px
						margin-bottom: 57%
						margin-left: -20px

						@media screen and (max-width: 1100px)
							margin-bottom: 12%;
							margin-left: 4px;

						@media screen and (max-width: 968px)
							margin-bottom: 3%;
